                               /* Görkem Araz */
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Lütfen ilk Yaşı girin:");
        int yas=20scanner.nextInt();
        switch(yas) {
          case 0:
             System.out.println("Hello World Baby");
         break;
            case 1:
             System.out.println("Hayata Hosgeldin");
             break;
            case 12:
                System.out.println("Ergenlige Hosgeldin");
                break;
            case 20:
                System.out.println("Yetiskinlige Hosgeldin");
                break;
      }

    }
}
